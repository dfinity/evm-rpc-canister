{
  "canisters": {
    "evm_rpc": {
      "candid": "candid/evm_rpc.did",
      "type": "rust",
      "package": "evm_rpc",
      "declarations": {
        "bindings": ["did", "mo"],
        "output": "lib/motoko/src/declarations"
      },
      "pullable": {
        "dependencies": [],
        "wasm_url": "https://github.com/dfinity/evm-rpc-canister/releases/latest/download/evm_rpc_dev.wasm.gz",
        "init_guide": "Number of nodes in the subnet, e.g. '(record {nodesInSubnet = 28})'"
      }
    },
    "evm_rpc_fiduciary": {
      "candid": "candid/evm_rpc.did",
      "type": "rust",
      "package": "evm_rpc"
    },
    "e2e_rust": {
      "dependencies": ["evm_rpc", "evm_rpc_fiduciary"],
      "candid": "e2e/rust/e2e_rust.did",
      "type": "rust",
      "package": "e2e"
    },
    "e2e_motoko": {
      "dependencies": ["evm_rpc", "evm_rpc_fiduciary"],
      "type": "motoko",
      "main": "e2e/motoko/Main.mo"
    }
  },
  "defaults": {
    "build": { "packtool": "npm run --silent sources" }
  },
  "version": 1
}
